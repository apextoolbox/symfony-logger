{
    "name": "apextoolbox/symfony-logger",
    "description": "Apex Toolbox Symfony Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "logging", "analytics", "apextoolbox", "errors"],
    "homepage": "https://apextoolbox.com",
    "authors": [
        {
            "name": "Apex Toolbox",
            "email": "spintokha@apextoolbox.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ApexToolbox\\SymfonyLogger\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ApexToolbox\\SymfonyLogger\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0",
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-foundation": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-client": "^5.4 || ^6.0 || ^7.0",
        "symfony/console": "^5.4 || ^6.0 || ^7.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0 || ^7.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "monolog/monolog": "^2.0 || ^3.0",
        "symfony/yaml": "^7.0 || ^6.0 || ^5.4",
        "ramsey/uuid": "^4.7",
        "symfony/uid": "^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "symfony/phpunit-bridge": "^5.4 || ^6.0 || ^7.0",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
